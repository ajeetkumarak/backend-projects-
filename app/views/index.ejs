<%- include("header") -%>

    <h1><%= listTitle %></h1>

<ul>
  <% for(let i = 0; i < newItems.length; i++){ %>
  <li><%= newItems[i] %></li>
  <% } %>
</ul>

<form action="/" method="post">
  <input
    type="text"
    name="newItem"
    placeholder="Add new item"
    autocomplete="off"
  />
  <button type="submit" name="button" value="<%= listTitle %>">Add</button>
</form>

<%- include("footer") -%>
